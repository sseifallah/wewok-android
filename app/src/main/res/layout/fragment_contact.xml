<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.home.contact.ContactFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.11"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline13H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.13"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline18H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.13"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline30H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.26"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline32H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.32"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline35H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.33"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline80H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.78"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline93H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.93"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline105H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="1.05"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline05V"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline15V"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.15"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline85V"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.85"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline38V"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.38"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline62V"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.62"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline95V"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/shape_rounded_bottom_corners"
        app:layout_constraintBottom_toTopOf="@id/guideline11H"/>


    <TextView
        android:id="@+id/txt_wok"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="Contact"
        android:textColor="@color/white"
        android:textSize="26sp"
        android:fontFamily="@font/nunito_bold"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/guideline11H"/>





    <ImageView
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:elevation="3dp"
        android:src="@drawable/logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/guideline18H"
        app:layout_constraintBottom_toTopOf="@id/guideline30H"/>


    <TextView
        android:id="@+id/txt_adresse"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/adresse"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:fontFamily="@font/nunito_bold"
        android:gravity="center"
        android:elevation="3dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@id/guideline05V"
        app:layout_constraintEnd_toEndOf="@id/guideline95V"
        app:layout_constraintTop_toBottomOf="@id/guideline32H"/>

    <Button
        android:id="@+id/btn_tlf"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/button_login"
        android:layout_marginTop="25dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:elevation="3dp"
        android:text="@string/numero"
        android:textAllCaps="false"
        android:padding="1dp"
        android:clickable="false"
        android:textSize="14sp"
        android:fontFamily="@font/nunito_semibold"
        app:layout_constraintStart_toStartOf="@id/guideline15V"
        app:layout_constraintEnd_toEndOf="@id/guideline85V"
        app:layout_constraintTop_toBottomOf="@+id/txt_adresse"
        app:layout_constraintHeight_percent="0.075"/>

    <Button
        android:id="@+id/btn_mail"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/button_login"
        android:layout_marginTop="25dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:elevation="3dp"
        android:text="contact@wewok.net"
        android:textAllCaps="false"
        android:padding="1dp"
        android:textSize="14sp"
        android:clickable="false"
        android:fontFamily="@font/nunito_semibold"
        app:layout_constraintStart_toStartOf="@id/guideline15V"
        app:layout_constraintEnd_toEndOf="@id/guideline85V"
        app:layout_constraintTop_toBottomOf="@+id/btn_tlf"
        app:layout_constraintHeight_percent="0.075"/>


